# key.env.example

# OpenAI API Key
OPENAI_API_KEY=your_api_key_here

# MongoDB Connection String
MONGODB_URI=your_mongodb_connection_string_here

# Port number for the local server (optional, default is usually 3000 for React apps)
PORT=3001

# Auth0 Configuration for React App
REACT_APP_AUTH0_DOMAIN=your_react_app_auth0_domain_here
REACT_APP_AUTH0_CLIENT_ID=your_react_app_auth0_client_id_here

# Auth0 Configuration for Backend Server
AUTH0_DOMAIN=your_backend_auth0_domain_here
AUTH0_CLIENT_ID=your_backend_auth0_client_id_here
AUTH0_CLIENT_SECRET=your_auth0_client_secret_here
